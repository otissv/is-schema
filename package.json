{
  "name": "is-schema",
  "version": "0.1.0",
  "description": "Checks if object literal is the correct schema",
  "main": "./lib/index.js",
  "keywords": [
    "schema",
    "validation",
    "object"
    "literal",
    "JSON"
  ],
  "scripts": {
    "build": "webpack",
    "start": "webpack --config --watch || true",
    "flow": "flow check; [[ $?=0||$?=2 ]]",
    "flow:watch": "watch 'npm run flow' ./src",
    "lint": "eslint --quiet './' && echo 'Completed:' && date || true && echo 'Completed:' && date",
    "lint:watch": "watch 'npm run lint' ./",
    "tape": "babel-node test-runner.js | tap-min  && date || true && date",
    "tape:spec": "babel-node test-runner.js | tap-difflet || true && date",
    "test": "npm run tape",
    "test:spec": "npm run tape:spec",
    "test:watch": "watch 'npm run tape' ./"
  },
  "author": "Otis Virginie",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "~5.0.0",
    "babel-istanbul": "~0.6.1",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-flow-strip-types": "~6.7.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-2": "~6.5.0",
    "deep-freeze": "0.0.1",
    "eslint": "~2.2.0",
    "eslint-config-standard": "~5.1.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-flowtype": "~2.1.2",
    "eslint-plugin-promise": "~1.1.0",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~1.3.2",
    "faucet": "0.0.1",
    "flow-bin": "~0.22.1",
    "flow-loader": "~1.0.2",
    "glob": "~7.0.3",
    "json-loader": "~0.5.4",
    "nyc": "~6.0.0",
    "plugin": "~0.3.3",
    "tap": "~5.7.0",
    "tap-difflet": "~0.4.0",
    "tap-min": "~1.1.0",
    "tape": "~4.5.1",
    "watch": "~0.17.1",
    "webpack": "~1.12.14"
  },
  "nyc": {
    "include": [
      "./src/**/*.js"
    ]
  }
}
